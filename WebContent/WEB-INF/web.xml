<?xml version="1.0" encoding="UTF-8"?><web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="WebApp_ID" version="2.4" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
    <display-name>CallEbayApi</display-name>
    <servlet>
        <description/>
        <display-name>GetLists</display-name>
        <servlet-name>GetLists</servlet-name>
        <servlet-class>com.ebay.glass.GetLists</servlet-class>
    </servlet>
    <servlet>
        <description/>
        <display-name>GetTime</display-name>
        <servlet-name>GetTime</servlet-name>
        <servlet-class>com.ebay.glass.GetTime</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetLists</servlet-name>
        <url-pattern>/GetLists</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetTime</servlet-name>
        <url-pattern>/GetTime</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>default.html</welcome-file>
        <welcome-file>default.htm</welcome-file>
        <welcome-file>default.jsp</welcome-file>
    </welcome-file-list>
</web-app>
